    "@graphql-yoga/subscription": "5.0.1",
    "@n1ru4l/graphql-live-query": "0.10.0",
    "@n1ru4l/in-memory-live-query-store": "0.10.0",
    "graphql": "16.9.0"
  },
  "devDependencies": {
