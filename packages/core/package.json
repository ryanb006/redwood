{
  "name": "@redwoodjs/core",
  "version": "7.0.0",
  "description": "Foundational packages and config required to build RedwoodJS.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redwoodjs/redwood.git",
    "directory": "packages/core"
  },
  "license": "MIT",
